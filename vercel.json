{
  "version": 2,
  "builds": [
    { "src": "api/*.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/latest", "dest": "/api/latest.js" },
    { "src": "/search", "dest": "/api/search.js" },
    { "src": "/detail", "dest": "/api/detail.js" },
    { "src": "/watch", "dest": "/api/watch.js" }
  ]
}
